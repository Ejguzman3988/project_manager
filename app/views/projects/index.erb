<h1>List of All Projects.</h1>
<nav>
    <a href="/projects/new">Create New Project</a>
</nav>
<ul>
    <% @projects.each do |project| %>
        <li><a href="/projects/<%=project.id%>"><%= project.name %></a> created by <a href="/users/<%=project.user.id%>"><%= project.user.name %></a></li>
    <% end %>
</ul>